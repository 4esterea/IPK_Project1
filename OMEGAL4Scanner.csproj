<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net8.0</TargetFramework>
    <AssemblyName>ipk-l4-scan</AssemblyName>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <SatelliteAssembliesForSelfContained>false</SatelliteAssembliesForSelfContained>
    <NeutralResourcesLanguage>en</NeutralResourcesLanguage>
    <PublishSingleFile>True</PublishSingleFile>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="System.CommandLine" Version="2.0.0-beta4.22272.1" />
    <EmbeddedResource Remove="Resources\*.resx" />
    <None Remove="cs\**\*;de\**\*;es\**\*;fr\**\*;it\**\*;ja\**\*;ko\**\*;pl\**\*;pt-BR\**\*;ru\**\*;tr\**\*;zh-Hans\**\*;zh-Hant\**\*;" />
  </ItemGroup>
  
  

</Project>